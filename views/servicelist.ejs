        <!-- listed -->
        <div class="row">
                <% images.forEach(function(images){ %>
                    <div class="col-lg-4">
                        <figure class="figure">
                            <img src="images/<%= images.imageName %>" class="figure-img img-fluid img-rounded" alt="<%= images.imageName %>">
                            <figcaption class="figure-caption"><%= images.servicetitle %></figcaption>
                            <!-- <small>Upload by: <%= images.user_id %></small> -->
                            <small>Price: <%= images.serviceprice %></small>
                            <small>Description: <%= images.servicedescription %></small>
                            <small>Location: <%= images.servicelocation %></small>

                        </figure>
                    </div>
                <% }); %>
            </div>
