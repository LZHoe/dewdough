<!-- service listing -->
        <div class = "container">
            <h4> My Service Listing</h4>
            <table> 
                <tr> 
                    <th>
                        <button type="button" id="add-service">Add Service Listing</button>
                    </th>
                <!-- </tr>
            </table> -->

            <th>

            <!-- service listing form -->
        <div class="show-onclick form-group">
            <form action="/images" method="post" enctype="multipart/form-data">
            <div class ="col-lg-3">
                <div class="form-group">
                            <!-- <label for="serviceimage1">First Name <span>*</span></label> -->
                            <input type="file" id="serviceimage1" name="serviceimage1" required>                
                </div>
            </div>

            <div class ="col-lg-3">
                    <div class="form-group">
                                <!-- <label for="serviceimage1">First Name <span>*</span></label> -->
                                <input type="file" id="serviceimage2" name="serviceimage2">                
                    </div>
            </div>

            <div class ="col-lg-3">
                <div class="form-group">
                            <!-- <label for="serviceimage1">First Name <span>*</span></label> -->
                            <input type="file" id="serviceimage3" name="serviceimage3">                
                </div>
            </div>

            <div class ="col-lg-3">
                    <div class="form-group">
                                <!-- <label for="serviceimage1">First Name <span>*</span></label> -->
                                <input type="file" id="serviceimage4" name="serviceimage4">                
                    </div>
            </div>
            
        
            <div class="col-lg-6">
                <div class="form-group">
                    <label for="servicetitle">Listing Title:  <span>*</span></label>
                    <input type="text" class="form-control" id="servicetitle" aria-describedby="name" placeholder="Listing Tile">
                </div>
            </div>

            <div class="col-lg-6">
                <div class="form-group">
                    <label for= "serviceprice">Price: <span>*</span></label>
                    <input type="number" required name="serviceprice" min="0" value="0" step=".01"
                    class="form-control" id="serviceprice" aria-describedby="serviceprice" placeholder="$0.00">
                </div>
            </div>

            <div class="col-lg-6">
                <div class="form-group">
                        <label for="servicedescription">Item Description: <span>*</span></label>
                        <textarea id="servicedescription" name="servicedescription" rows="5" cols="40" 
                        class="form-control" placeholder="Service Description" required=""></textarea>
                </div>
            </div>

            <div class="col-lg-6">
                    <div class="form-group">
                        <label for="servicecategory">Category <span>*</span></label>
                        <select class="selectpicker" id="servicecategory">
                            <option> <value="Dog Grooming">Dog Grooming </value></option>
                            <option> <value="Dog Daycare">Dog Daycare </value></option>
                            <option> <value="Dog Boarding">Dog Boarding </value></option>
                            <option> <value="Cat Grooming">Cat Grooming </value></option>
                            <option> <value="Cat Daycare">Cat Daycare </value></option>
                            <option> <value="Cat Boarding">Cat Boarding </value></option>


                        </select>
                    </div>
            </div>

            <div class="col-lg-6">
                <div class="form-group">
                        <label for="servicelocation">Location:  <span>*</span></label>
                        <input type="text" class="form-control" id="servicelocation" aria-describedby="service-location" placeholder="Location">
                </div>
            </div>

            <div class="col-lg-6">
                    <button type="submit" id="servicebutton" value="submit" class="btn subs_btn form-control">Submit</button>
            </div>
    </form>
        </div>
        </div>
    </th>
   
    </tr>

</table>
