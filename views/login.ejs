<!DOCTYPE html>
<html lang="en">
    <head>
        <% include partial/head %>
    </head>
    <body>     
        <% include partial/header %>
        
        <!--================Categories Banner Area =================-->
        <section class="solid_banner_area">
            <div class="container">
                <div class="solid_banner_inner">
                    <h3>Login</h3>
                    <ul>
                        <li><a href="#">Home</a></li>
                        <li><a href="track.html">Login</a></li>
                    </ul>
                </div>
            </div>
        </section>
        <!--================End Categories Banner Area =================-->
        <p><%= message %></p>
        <!-- <div>
            message: <%= message %>
        <script>
            var message = <%= message %>;
            console.log(message);
        </script>
        </div> -->
        <!--================login Area =================-->
        <section class="login_area p_100">
                <div class="container">
                    <div class="login_inner">
                        <div class="row">
                            <div class="col-lg-12">
                                <div class="login_title" align="Center">
                                    <h2>log in your account</h2>
                                    <p>Log in to your account to discovery all great features in this template.</p>
                                </div>
                                <form class="login_form row" action = '/login' method = 'POST'>
                                    <div class="col-lg-12 form-group">
                                        <input class="form-control" name="email" type="email" placeholder="Email">
                                    </div>
                                    <div class="col-lg-12 form-group">
                                        <input class="form-control" name="password" type="password" placeholder="Password">
                                    </div>
                                    <div class="col-lg-12 form-group">
                                        <h4><a href="/signup">Sign Up Here</h4></a>
                                    </div>
                                    <div class="col-lg-12 form-group">
                                        <button type="submit" value="submit" class="btn update_btn form-control">Login</button>
                                    </div>
                                </form>
                            </div>
                        </div>
                    </div>
                </div>
            </section>
            <!--================End login Area =================-->
        
        <% include partial/footer %>
        <!--================End Footer Area =================-->
        
        
        
        
        <!-- jQuery (necessary for Bootstrap's JavaScript plugins) -->
        <script src="js/jquery-3.2.1.min.js"></script>
