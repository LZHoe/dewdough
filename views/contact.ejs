        <!--================Contact Area =================-->
        <section class="contact_area p_100">
            <div class="container">
                <div class="contact_title">
                    <h2>Get in Touch</h2>
                    <p>Got a burning question? Find out how to contact us here!</p>
                </div>
                <div class="row contact_details">
                    <div class="col-lg-4 col-md-6">
                        <div class="media">
                            <div class="d-flex">
                                <i class="fa fa-map-marker" aria-hidden="true"></i>
                            </div>
                            <div class="media-body">
                                <p>180 Ang Mo Kio Avenue 8,</p>
                                <p>569830</p>
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-4 col-md-6">
                        <div class="media">
                            <div class="d-flex">
                                <i class="fa fa-phone" aria-hidden="true"></i>
                            </div>
                            <div class="media-body">
                                <a href="tel:+6584691353">+65 8469 1353</a>
                                <a href="tel:+6593802522">+65 9380 2522</a>
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-4 col-md-6">
                        <div class="media">
                            <div class="d-flex">
                                <i class="fa fa-envelope" aria-hidden="true"></i>
                            </div>
                            <div class="media-body">
                                <a href="mailto:zhenghan@support.com">zhenghan@support.com</a>
                                <a href="mailto:desmond@support.com">desmond@support.com</a>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="contact_form_inner">
                    <h3>Drop a Message</h3>
                    <form class="contact_us_form row" action="/ask" method="post"  >
                        <div class="form-group col-lg-6">
                            <input type="text" class="form-control" id="name" name="name" required pattern="[^0-9]+" placeholder="Full Name *">
                        </div>
                        <div class="form-group col-lg-6">
                            <input type="email" class="form-control" id="email" required name="email" placeholder="Email Address *">
                        </div>

                        <div class="form-group col-lg-12">
                            <textarea class="form-control" name="question" id="question" required rows="1" placeholder="Type Your Message..."></textarea>
                        </div>
                        <div class="form-group col-lg-12">
                            <button type="submit" value="submit" onclick ="myFunction()" action="/ask"  class="btn update_btn form-control">Send Message</button>
                        </div>
                    </form>
                </div>
            </div>
        </section>
